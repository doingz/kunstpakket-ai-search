{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search-endpoint.js"
    }
  ],
  "headers": [
    {
      "source": "/widget.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}

